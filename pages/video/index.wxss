.fav-hint {
  position: fixed;
  top: 18rpx;
  right: 25rpx;
  padding: .5em 1em;
  width: 13em;
  border-radius: 4px;
  background-color: #000;
  color: #fff;
  font-size: 20rpx;
  line-height: 1.6em;
  text-align: left;
  z-index: 2001;
  box-shadow: 0 0 1px rgba(0,0,0,.35),0 2px 4px rgba(0,0,0,.15);
  animation: dotFlash 1s ease-in-out 10s forwards;
}

.fav-hint.en {
  width: 14em;
}

.fav-hint::before {
  top: -14rpx;
  border-left: 15rpx solid transparent;
  border-right: 15rpx solid transparent;
  border-bottom: 15rpx solid #000;
  z-index: 2001;
}

.fav-hint::after,.fav-hint::before {
  content: "";
  position: absolute;
  right: calc(71.33px - 25rpx);
  width: 0rpx;
  height: 0rpx;
  transform: translateX(50%);
}

.fav-hint::after {
  top: -16rpx;
  border-left: 16rpx solid transparent;
  border-right: 16rpx solid transparent;
  border-bottom: 16rpx solid #000;
}

.close-fav-hint {
  position: absolute;
  right: .8em;
  top: 67%;
  width: 1.2em;
  height: 1.2em;
  border-radius: 50%;
  background-color: #f00303;
  transform: translateY(-50%);
}

.close-fav-hint::after,.close-fav-hint::before {
  content: "";
  position: absolute;
  left: 50%;
  top: 50%;
  width: .7em;
  height: 4rpx;
  border-radius: 2rpx;
  background-color: #fff;
}

.close-fav-hint::before {
  transform: translate(-50%,-50%) rotate(45deg);
}

.close-fav-hint::after {
  transform: translate(-50%,-50%) rotate(-45deg);
}

@-webkit-keyframes bounce {
  0%,5% {
      transform: translate3d(0,2px,0);
  }

  100% {
      transform: translate3d(0,-2px,0);
  }
}

@keyframes bounce {
  0%,5% {
      transform: translate3d(0,2px,0);
  }

  100% {
      transform: translate3d(0,-2px,0);
  }
}

