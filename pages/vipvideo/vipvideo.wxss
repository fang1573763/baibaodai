
.nav-back {
  position: fixed;
  left: 0;
  top: 50rpx;
  z-index: 20;
  height: 40rpx;
  line-height: 40rpx;
  padding: 10rpx 15rpx;
}

.nav-back-hover {
  background-color: transparent;
}

.nav-back {
  color: #fff;
}

.container1 {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx;
  box-sizing: border-box;
}

.topbg {
  top: 0;
  left: 0;
  width: 100%;
  height: 540px;
  position: fixed;
  margin-top: 200rpx;
  background-position: center top;
  background-size: contain;
  z-index: -1;
}

.header {
  width: 100%;
  color: #ff4444;
  font-size: 42rpx;
  font-weight: bold;
  margin-top: 50rpx;
  text-align: center;
  padding: 26rpx 0 30rpx;
}

.header .beta {
  font-size: 24rpx;
  color: #806128;
  padding: 0 2rpx;
  vertical-align: super;
  line-height: 20rpx;
  font-weight: bold;
  border-radius: 6rpx;
  background-image: -webkit-linear-gradient(bottom, red, #fd8403, yellow);
  background-clip: text;
  -webkit-text-fill-color: transparent;
}

.input1 {
  width: 75vw;
  color: white;
  margin-top: 45rpx;
  padding: 25rpx 40rpx;
  border-radius: 60rpx;
  background-color: #ff4444;
}

.input-placeholder {
  color: #fff;
}

.videos-container {
  width: 100%;
  display: flex;
  padding: 20rpx;
  flex-wrap: wrap;
  box-sizing: border-box;
  justify-content: flex-start;
}

.videos-container>view {
  flex-basis: 30%;
  margin: 10rpx;
  padding: 16rpx 12rpx;
  font-size: 30rpx;
  border-radius: 8rpx;
  text-align: center;
  color: #fff;
  background: #ff4444;
  box-sizing: border-box;
  transition: 0.5s all;
}

.videos-container>view.vv-hover {
  color: #fff;
  background: #ee8787;
}

.videos-container>view.popple {
  animation: freebubble 1.8s ease-in-out infinite alternate;
}

.note {
  color: #aaa;
  font-size: 24rpx;
  margin-top: 10rpx;
}

.ad-container {
  padding: 20rpx;
  overflow: hidden;
  border-radius: 20rpx;
  width: calc(100% - 40rpx);
}

.submit {
  width: 90%;
  color: #ff4444;
  flex-basis: 100%;
  margin: 25rpx 0 40rpx;
  border-radius: 80rpx;
  letter-spacing: 20rpx;
  background: linear-gradient(145deg, #f8c463, #ffbe48);
  transition: 0.6s all;
}

.submit-hover {
  background: linear-gradient(145deg, #f8c463, #f8c463);
}

.submit::after {
  border: none;
}

@-webkit-keyframes freebubble {
  0% {
    background: #1f1f1f;
  }

  100% {
    background: #3d3d3d;
  }
}

@keyframes freebubble {
  0% {
    background: #1f1f1f;
  }

  100% {
    background: #3d3d3d;
  }
}

.none-data {
  font-size: 26rpx;
  filter: invert(90%);
  padding-top: 200rpx;
  margin-bottom: 40rpx;
  background-position: center 30rpx;
}

.none-data {
  width: 100%;
  color: #a9bbc5;
  text-align: center;
  padding-top: 230rpx;
  background: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-position: center 50rpx;
}
