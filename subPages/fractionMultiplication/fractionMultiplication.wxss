.safe-area-inset-bottom {
    padding-bottom: env(safe-area-inset-bottom);
}

.radius-box {
    background-color: #fff;
    border-radius: 12rpx;
    box-shadow: 0 4rpx 10rpx 0 rgba(0,0,0,.05);
    margin: 24rpx;
    overflow: auto;
    padding: 24rpx;
}

.radius-box:last-of-type {
    margin-bottom: 0;
}

.list-title {
    color: #303133;
    font-size: 36rpx;
    font-weight: 700;
    margin-bottom: 24rpx;
    margin-top: 68rpx;
    padding-left: 48rpx;
    padding-right: 48rpx;
}

.fractionMultiplication {
    background: #f5f5f5;
    min-height: calc(100vh - 88rpx);
    overflow-x: hidden;
    overflow-y: auto;
    padding-top: 88rpx;
}

.fractionMultiplication .divider {
    align-items: center;
    background-color: #202122;
    display: flex;
    flex-direction: row;
    height: 2rpx;
    margin: 12rpx 0;
}

.fractionMultiplication .topArea {
    align-items: center;
    background-color: #f5f5f5;
    display: flex;
    justify-content: space-between;
    left: 0;
    padding: 0 24rpx;
    position: fixed;
    right: 0;
    top: 0;
    z-index: 1000;
}

.fractionMultiplication .modal-title {
    background-color: #ff4444;
    border-top-left-radius: 20rpx;
    border-top-right-radius: 20rpx;
    color: #fff;
    padding: 24rpx 26rpx;
}

.fractionMultiplication .modal-list {
    height: 50vh;
    overflow-y: auto;
}

.fractionMultiplication .content,.fractionMultiplication .list {
    background-color: #fff;
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    margin-bottom: 24rpx;
    padding: 0 12rpx;
}

.fractionMultiplication .content .item,.fractionMultiplication .list .item {
    align-items: center;
    box-sizing: border-box;
    display: inline-flex;
    font-size: 48rpx;
    padding: 48rpx 24rpx;
}

.fractionMultiplication .content .item .index,.fractionMultiplication .list .item .index {
    color: #c0c4cc;
    font-size: 32rpx;
    text-align: left;
    width: 60rpx;
}

.fractionMultiplication .content .item .number,.fractionMultiplication .list .item .number {
    padding: 0 2rpx;
    text-align: center;
}

.fractionMultiplication .content .item .symbol,.fractionMultiplication .list .item .symbol {
    align-items: center;
    display: flex;
    justify-content: center;
    padding: 0 24rpx;
}

.fractionMultiplication .content .item .result-wrap,.fractionMultiplication .list .item .result-wrap {
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.fractionMultiplication .content .item .result-wrap .result,.fractionMultiplication .list .item .result-wrap .result {
    align-items: center;
    background: #fff;
    border: 2rpx solid #c0c4cc;
    border-radius: 8rpx;
    display: inline-flex;
    justify-content: center;
    min-height: 72rpx;
    min-width: 88rpx;
    padding: 0 8rpx;
}

.fractionMultiplication .content .item .result-wrap .result.active,.fractionMultiplication .list .item .result-wrap .result.active {
    background-color: #fae3d9;
    border: 2rpx solid #000;
}

.fractionMultiplication .content .item .icon,.fractionMultiplication .list .item .icon {
    margin-left: 12rpx;
}

.fractionMultiplication .submit-btn {
    padding: 0 24rpx;
}

.fractionMultiplication .share {
    padding: 24rpx;
}

.fractionMultiplication .share .icon {
    margin-right: 8rpx;
}

.fractionMultiplication .share .share-over {
    background: transparent;
}

.fractionMultiplication .footer {
    bottom: 0;
    left: 0;
    position: fixed;
    right: 0;
    z-index: 10075;
}

.fractionMultiplication .answer-result .line {
    align-items: center;
    display: flex;
}